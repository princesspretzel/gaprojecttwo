    <div class='form-group'>
    <input name='genre' class='form-control' id='genre' placeholder='genre'></input>
    <input name='city' class='form-control' id='city' placeholder='city'></input>
    <button id='enter' class='btn btn-primary'>go</button>
    </div>

    <ul id='ul'>

    </ul>

<iframe width='100%' id='iframe' style='border-style: none' border='0' cellspacing='0' height='166' scrolling='no' frameborder='no'>

</iframe>
    <%= @server_embed %>

<%= @responce %>

