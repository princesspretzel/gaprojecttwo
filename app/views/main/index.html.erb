<div id="page-top" class="row">

  <img id="logo" class="col-sm-2" src="http://icons.iconarchive.com/icons/dan-wiersma/solar-system/512/Uranus-icon.png" alt="">

  <div class='col-xs-3 pull-right margin-top'>
    <input name='genre' id='genre' class="form-control" placeholder='genre'></input>
    <br>
    <input name='city' class='form-control' id='city' placeholder='city'></input>
    <button id='enter' class='btn btn-primary'>go</button>
  </div>

</div>

<div id="player" class="col-xs-8 col-xs-offset-2">

</div>


<footer class="col-xs-12">
  
</footer>



<script type="text/javascript">
    $('#enter').on('click', function(){
        $.ajax({
            url: '/test',
            method: 'post',
            dataType: 'json',
            data: { genre: $('#genre').val(), city: $('#city').val() }
        })
        .success( function(data){
            console.log(data);
            $('#ul').empty();
            for (var i=0;i<10;i++){
                var foo = $('<iframe width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/' + data[i].id +'&color=01DFA5&show_comments=false&show_artwork=true&show_playcount=false&liking=false&theme_color=01DFA5&sharing=false&buying=false&show_user=false&show_artwork=false"></iframe>');
                $('#ul').append(foo)
                $('#ul').append('<img src="' + data[i].waveform_url + '">')
            }

        })
    })

</script>    