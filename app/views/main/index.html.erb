<script type="text/javascript">
    $('#enter').on('click', function(){
        $.ajax({
            url: '/test',
            method: 'post',
            dataType: 'json',
            data: { genre: $('#genre').val(), city: $('#city').val() }
        })
        .success( function(data){
            console.log(data);
            $('#ul').empty();
            for (var i=0;i<10;i++){
                var foo = $('<iframe width="100%"  style="border-style: none" border="0" cellspacing="0" height="166" scrolling="no" frameborder="no" src="//w.soundcloud.com/player/?url=//api.soundcloud.com/tracks/' + data[i].id +'&color=01DFA5&show_comments=false&show_artwork=true&show_playcount=false&liking=false&theme_color=01DFA5&sharing=false&buying=false&show_user=false&show_artwork=false"></iframe>');
                $('#ul').append(foo)
            }
        })
    })
    $('#iframe').html(<%= @responce %>)

</script>    